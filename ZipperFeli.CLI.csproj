<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
	<PackAsTool>true</PackAsTool>
	<ToolCommandName>zipper</ToolCommandName>
	<PackageOutputPath>./nupkg</PackageOutputPath>
	<Version>1.0.0</Version>
	<Description>A tool for zip and un-zip files</Description>
	<PackageProjectUrl>https://github.com/01-Feli/ZipperFeli</PackageProjectUrl>
	<RepositoryUrl>https://github.com/01-Feli/ZipperFeli</RepositoryUrl>
	<PackageTags>Zip/UnZip files, zip, unzip</PackageTags>
	<PackageId>ZipperFeli</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.9.0-preview1" />
  </ItemGroup>

</Project>
